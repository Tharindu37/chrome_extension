{
  "manifest_version": 3,
  "name": "Image and Text Extractor",
  "version": "1.0",
  "description": "Extract images and text from a webpage.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
